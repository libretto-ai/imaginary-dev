{
  "name": "promptify",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build",
    "build:example": "nx run-many --target=build:example",
    "clean": "nx run-many --target=clean",
    "full-clean": "nx run-many --target=full-clean && rm -rf node_modules",
    "test": "nx run-many --target=test",
    "coverage": "nx run-many --target=coverage",
    "deploy-site": "nx run-many --target=deploy-site",
    "prettier": "prettier -w ."
  },
  "workspaces": [
    "prompt-engine",
    "example-clients/*",
    "transformers/*",
    "nextjs-util",
    "util",
    "vsc-extension"
  ],
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@nrwl/nx-cloud": "15.2.1",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^16.18.21",
    "jest": "^29.3.1",
    "nx": "15.8.6",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "json5": "^2.2.3",
    "string-to-file-stream": "^1.3.0"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "9.5.1"
  }
}
