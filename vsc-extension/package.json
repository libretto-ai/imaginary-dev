{
  "name": "imaginary-programming",
  "displayName": "Imaginary Programming",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "panel": [
        {
          "id": "currentfunctions",
          "title": "Current functions",
          "icon": "resources/favicon-32x32.png"
        }
      ]
    },
    "views": {
      "currentfunctions": [
        {
          "id": "imaginary.currentfunctions",
          "name": "view: currentfunctions",
          "type": "webview"
        }
      ],
      "explorer": [
        {
          "id": "functions",
          "name": "Imaginary Function List"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test-disabled": "node ./dist/test/runTest.js"
  },
  "dependencies": {
    "@imaginary-dev/typescript-transformer": "*",
    "@imaginary-dev/util": "*",
    "html-template-tag": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "recoil-sync": "^0.2.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.76.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^16.18.21",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "typescript": "^4.9.5"
  }
}
