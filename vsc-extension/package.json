{
  "name": "@imaginary-dev/imaginary-programming-extension",
  "displayName": "Imaginary Programming",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./dist/vsc-extension/src/extension.js",
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "panel": [
        {
          "id": "currentfunctions",
          "title": "Current functions",
          "icon": "resources/favicon-32x32.png"
        }
      ],
      "activitybar": [
        {
          "id": "imaginary",
          "title": "Imaginary Programming",
          "icon": "resources/favicon-32x32.png"
        }
      ]
    },
    "views": {
      "imaginary": [
        {
          "id": "imaginary.inputs",
          "name": "Test Inputs",
          "type": "webview"
        },
        {
          "id": "functions",
          "name": "Imaginary Function List"
        }
      ],
      "currentfunctions": [
        {
          "id": "imaginary.currentfunctions",
          "name": "Inputs & Outputs",
          "type": "webview"
        }
      ],
      "explorer": []
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "webpack && ttsc -p ./",
    "coverage": "jest --coverage",
    "watch": "ttsc -watch -p ./",
    "test": "jest",
    "lint": "eslint src src-* --ext ts"
  },
  "dependencies": {
    "@imaginary-dev/runtime": "*",
    "@imaginary-dev/typescript-transformer": "*",
    "@imaginary-dev/util": "*",
    "@vscode/codicons": "^0.0.32",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "html-template-tag": "^4.0.1",
    "immer": "^9.0.21",
    "json-schema": "^0.4.0",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "recoil-sync": "^0.2.0",
    "stream-http": "^3.2.0",
    "ts-essentials": "^9.3.1",
    "ts-loader": "^9.4.2",
    "url": "^0.11.0",
    "webpack": "^5.76.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.18.21",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@types/vscode": "^1.76.0",
    "@types/vscode-webview": "^1.57.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.38.0",
    "glob": "^10.0.0",
    "jest": "^29.5.0",
    "jest-mock-vscode": "^1.3.0",
    "style-loader": "^3.3.2",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack-cli": "^5.0.1"
  }
}
